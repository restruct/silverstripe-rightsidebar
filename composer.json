{
    "name": "restruct/rightsidebar",
    "description": "Allows adding a right-hand sidebar in the CMS to hold options/fields for the currently edited record",
    "type": "silverstripe-vendormodule",
    "authors": [{
		"name": "Restruct",
		"email": "dev@restruct.nl"
	}],
	"license": "BSD-2-Clause",
    "require": {
        "silverstripe/cms": "~4.0",
        "silverstripe/framework": "~4.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Restruct\\RightSidebar\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "css",
            "js"
        ]
    }
}
